@inherits Palermo.BlazorMvc.ViewComponentBase

<h1>Counter</h1>

<p>Current count: @Model as of @Time</p>

<button class="btn btn-primary" @onclick="OnIncrement">Click me</button>

@code {

    public Action OnIncrement { get; set; }
    public int Model { get; set; }
    public string Time { get; set; }
}